<!-- Navbar -->
<nav class="navbar navbar-expand-lg navbar-dark bg-primary">
  {{!-- Brand Button --}}
  <a class="navbar-brand" href="/">
    <img src="assets/wiredLogo.png" width="auto" height="30" class="d-inline-block align-top" alt="Mongo Scraper">
    
  </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active">
        {{!-- Button to home --}}
        <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item">
        {{!-- Button to saved articals --}}
        <a class="nav-link" href="/articles">Saved Articals</a>
      </li>
    </ul>
  </div>
</nav>

<div id="wrapper">
  <h2 id='title'><u>Saved Articals</u></h2>
  <div id="articles">
    {{#each articles}}
      <div class='container'>
      
      <p><a href='{{this.link}}'><h3 data-id={{this._id}}>{{this.headline}}</h3></a></p>
      <p><h4>{{this.summary}}</h4></p>
      <p><img src="{{this.imageURL}}" alt="{{this.imageURL}}" width="50%" height='auto'></p>
      <p>{{this.articleDate}}</p>
      </div>
      <div>
       </div>
      <hr>

    {{/each}}
  </div>
  <div id="notes"></div>
</div>